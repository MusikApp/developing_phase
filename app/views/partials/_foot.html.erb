<footer class="footer">
<ul>
    <li>
      <%= link_to(root_path) do %>
        <i class="fas fa-home"></i>
      <% end  %>
    </li>
    <li>
      <%= link_to() do %>
        <i class="fas fa-compass"></i>
      <% end %>
    </li>
    <% if current_user %>
        <li>
            <%= image_tag current_user.avatar_thumbnail, :class => "navbar-thumbnail", :id => "navbar-thumbnail" %> 
        </li>
    <% else %>
        <li>
            <%= link_to(new_user_session_path) do %>
                <p class="nav-link">Sign In</p>
            <% end %>
        </li>
    <% end %>
</footer>